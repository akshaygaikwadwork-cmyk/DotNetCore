@model HttpClient_StockApps.Models.Stock
@{
    ViewBag.Title = "Home";
}
<h1>Home Page</h1>

<div class="box trading-panel">
    <div class="flex trading-panel-flex" >
        <div class="stock-price-panel">
            <div class="stock">
                <h1 class="stok-title">@Model.StockSymbol</h1>
            </div>
            <table>
                <tbody>
                    <tr>
                        <td>Current:</td>
                        <th>@Model.CurrentPrice.ToString("N")</th>
                    </tr>
                    <tr>
                        <td>Lowest:</td>
                        <th>@Model.LowestPrice.ToString("N")</th>
                    </tr>
                    <tr>
                        <td>Highest:</td>
                        <th>@Model.HighestPrice.ToString("N")</th>
                    </tr>
                    <tr>
                        <td>Open:</td>
                        <th>@Model.OpenPrice.ToString("N")</th>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
</div>